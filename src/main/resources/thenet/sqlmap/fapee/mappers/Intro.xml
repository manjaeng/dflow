<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="kr.co.thenet.fapee.intro.service.impl.IntroMapper">

	<select id="selectIntroFavoriteStyleList" resultType="egovMap">
		SELECT  idkey
			  , seq
			  , style
		  FROM FP_FavoriteStyle
		 ORDER BY seq
	</select>
	
	<select id="selectIntroSizeCount" resultType="egovMap" parameterType="egovMap">
		SELECT  count(*) as count
		  FROM FP_UserFilter
	</select>
	
</mapper>